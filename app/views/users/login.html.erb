<div id="content_title">
    <h2>Login</h2>
</div>
<div id="login_area">

    <%= form_tag do %>
        <%= label_tag :email %>:
        <%= text_field_tag :email%><br />

        <%= label_tag :password %>:
        <%= password_field_tag :password %><br />

        <%= submit_tag "Login" %>
    <% end %>

    <p> <a class="navlistitem" href="">Forgot Password?</a> </p>
</div>